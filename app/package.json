{
  "name": "app",
  "version": "0.0.0",
  "description": "oclif example Hello World CLI",
  "author": "Josh Yudaken @qix",
  "bin": {
    "app": "./bin/run"
  },
  "homepage": "https://github.com/qix/bluesqrl",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "qix/bluesqrl",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@atproto/did-resolver": "^0.1.0",
    "@atproto/lexicon": "^0.1.0",
    "@atproto/repo": "^0.1.0",
    "@atproto/uri": "^0.0.2",
    "@atproto/xrpc-server": "^0.2.0",
    "@google-cloud/bigquery": "^6.2.0",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "kafkajs": "^2.2.4",
    "sqrl": "../sqrl/packages/sqrl",
    "sqrl-jsonpath": "../sqrl/packages/sqrl-jsonpath",
    "sqrl-load-functions": "../sqrl/packages/sqrl-load-functions",
    "sqrl-redis-functions": "../sqrl/packages/sqrl-redis-functions",
    "sqrl-text-functions": "../sqrl/packages/sqrl-text-functions"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.17",
    "@types/chai": "^4",
    "@types/express": "^4.17.17",
    "@types/kafkajs": "^1.9.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.26",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "^2.8.8",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "app",
    "dirname": "app",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/qix/bluesqrl/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
